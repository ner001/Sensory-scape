{
  "files": [],
  "compileOnSave": true,
  "references": [
    {
      "path": "./tsconfig.app.json"
    }
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["components/*"],
      "@layouts/*": ["layouts/*"],
      "@pages/*": ["pages/*"]
    },
    "outDir": "out",
    "rootDir": "./src",
    "target": "es5",
    "allowJs": false,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": false,
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "composite": true,
    "tsBuildInfoFile": "./.tsbuildinfo",
  },
  "include": ["src"],
  "exclude": [
    "node_modules",
    "dist",
    "**/*.js"
  ]
}